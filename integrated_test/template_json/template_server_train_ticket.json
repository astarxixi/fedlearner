{
  "name": "!name",
  "federation_id": "!federation_id",
  "job_type": "nn_model",
  "role": "Leader",
  "sdk_version": "!sdk_version",
  "expire_time": "!expire_time",
  "public_params": {
    "spec": {
      "flReplicaSpecs": {
        "Master": {
          "pair": false,
          "template": {
            "spec": {
              "containers": [
                {
                  "env": [
                    {
                      "name": "DATA_SOURCE",
                      "value": "it-data-join-job-"
                    }
                  ],
                  "image": "artifact.bytedance.com/fedlearner/fedlearner:",
                  "ports": [
                    {
                      "containerPort": 50051,
                      "name": "flapp-port"
                    }
                  ],
                  "command": [
                    "/app/deploy/scripts/trainer/run_trainer_master.sh"
                  ]
                }
              ]
            }
          }
        },
        "PS": {
          "pair": false,
          "template": {
            "spec": {
              "containers": [
                {
                  "image": "artifact.bytedance.com/fedlearner/fedlearner:",
                  "ports": [
                    {
                      "containerPort": 50051,
                      "name": "flapp-port"
                    }
                  ],
                  "command": [
                    "/app/deploy/scripts/trainer/run_trainer_ps.sh"
                  ]
                }
              ]
            }
          }
        },
        "Worker": {
          "pair": true,
          "template": {
            "spec": {
              "containers": [
                {
                  "env": [
                    {
                      "name": "CODE_KEY",
                      "value": "/app/integrated_test/code_key/criteo-train-2.tar.gz"
                    },
                    {
                      "name": "SAVE_CHECKPOINT_STEPS",
                      "value": "100"
                    },
                    {
                      "name": "SPARSE_ESTIMATOR",
                      "value": "True"
                    }
                  ],
                  "image": "artifact.bytedance.com/fedlearner/fedlearner:",
                  "ports": [
                    {
                      "containerPort": 50051,
                      "name": "flapp-port"
                    },
                    {
                      "containerPort": 50052,
                      "name": "tf-port"
                    }
                  ],
                  "command": [
                    "/app/deploy/scripts/wait4pair_wrapper.sh"
                  ],
                  "args": [
                    "/app/deploy/scripts/trainer/run_trainer_worker.sh"
                  ]
                }
              ]
            }
          }
        }
      }
    }
  },
  "private_params": null
}
